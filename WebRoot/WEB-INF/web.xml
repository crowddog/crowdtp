<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>webget</servlet-name>
    <servlet-class>web.webget</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>webget</servlet-name>
    <url-pattern>/webget</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ProcessRequest</servlet-name>
  	<servlet-class>com.sk.servlet.ProcessRequest</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProcessRequest</servlet-name>
  	<url-pattern>/travelplanning</url-pattern>
  </servlet-mapping>
</web-app>
